//-------------------------------------------------------------------------------------------------------------
_nBufferOutfeedTarget[1] := E_INSTANCE.OUTFEED_ROBOT_R1;
_nBufferOutfeedTarget[2] := E_INSTANCE.OUTFEED_ROBOT_R2;

BufferOutfeed.ProcessId              := E_INSTANCE.BUFFER_OUTFEED;
