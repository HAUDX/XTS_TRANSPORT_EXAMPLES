//-------------------------------------------------------------------------------------------------------------
ItfProcessCollector[Outfeed_R1.ProcessId].StationFirst  := 26;
ItfProcessCollector[Outfeed_R1.ProcessId].StationLast   := 37;

_nStnFrst                                 := ProcessCollector[Outfeed_R1.ProcessId].StationFirst;
